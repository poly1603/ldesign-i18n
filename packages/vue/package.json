{
  "name": "@ldesign/i18n-vue",
  "version": "4.0.0",
  "description": "Vue 3 integration for @ldesign/i18n - Powerful i18n solution with composition API, components, and directives",
  "keywords": [
    "i18n",
    "internationalization",
    "localization",
    "vue",
    "vue3",
    "typescript",
    "composition-api",
    "reactive"
  ],
  "author": "ldesign",
  "license": "MIT",
  "type": "module",
  "sideEffects": [
    "*.css",
    "*.vue"
  ],
  "exports": {
    ".": {
      "types": "./esm/index.d.ts",
      "import": "./esm/index.js",
      "require": "./cjs/index.cjs"
    },
    "./components": {
      "types": "./esm/components/index.d.ts",
      "import": "./esm/components/index.js",
      "require": "./cjs/components/index.cjs"
    },
    "./components/*": {
      "types": "./esm/components/*.d.ts",
      "import": "./esm/components/*.js",
      "require": "./cjs/components/*.cjs"
    },
    "./composables": {
      "types": "./esm/composables/index.d.ts",
      "import": "./esm/composables/index.js",
      "require": "./cjs/composables/index.cjs"
    },
    "./composables/*": {
      "types": "./esm/composables/*.d.ts",
      "import": "./esm/composables/*.js",
      "require": "./cjs/composables/*.cjs"
    },
    "./directives": {
      "types": "./esm/directives/index.d.ts",
      "import": "./esm/directives/index.js",
      "require": "./cjs/directives/index.cjs"
    },
    "./directives/*": {
      "types": "./esm/directives/*.d.ts",
      "import": "./esm/directives/*.js",
      "require": "./cjs/directives/*.cjs"
    },
    "./i18n-provider": {
      "types": "./esm/i18n-provider/index.d.ts",
      "import": "./esm/i18n-provider/index.js",
      "require": "./cjs/i18n-provider/index.cjs"
    },
    "./i18n-provider/*": {
      "types": "./esm/i18n-provider/*.d.ts",
      "import": "./esm/i18n-provider/*.js",
      "require": "./cjs/i18n-provider/*.cjs"
    },
    "./i18n-text": {
      "types": "./esm/i18n-text/index.d.ts",
      "import": "./esm/i18n-text/index.js",
      "require": "./cjs/i18n-text/index.cjs"
    },
    "./i18n-text/*": {
      "types": "./esm/i18n-text/*.d.ts",
      "import": "./esm/i18n-text/*.js",
      "require": "./cjs/i18n-text/*.cjs"
    },
    "./i18n-translate": {
      "types": "./esm/i18n-translate/index.d.ts",
      "import": "./esm/i18n-translate/index.js",
      "require": "./cjs/i18n-translate/index.cjs"
    },
    "./i18n-translate/*": {
      "types": "./esm/i18n-translate/*.d.ts",
      "import": "./esm/i18n-translate/*.js",
      "require": "./cjs/i18n-translate/*.cjs"
    },
    "./language-switcher": {
      "types": "./esm/language-switcher/index.d.ts",
      "import": "./esm/language-switcher/index.js",
      "require": "./cjs/language-switcher/index.cjs"
    },
    "./language-switcher/*": {
      "types": "./esm/language-switcher/*.d.ts",
      "import": "./esm/language-switcher/*.js",
      "require": "./cjs/language-switcher/*.cjs"
    },
    "./plugins": {
      "types": "./esm/plugins/index.d.ts",
      "import": "./esm/plugins/index.js",
      "require": "./cjs/plugins/index.cjs"
    },
    "./plugins/*": {
      "types": "./esm/plugins/*.d.ts",
      "import": "./esm/plugins/*.js",
      "require": "./cjs/plugins/*.cjs"
    },
    "./utils/*": {
      "types": "./esm/utils/*.d.ts",
      "import": "./esm/utils/*.js",
      "require": "./cjs/utils/*.cjs"
    },
    "./cjs/index.css": "./cjs/index.css",
    "./cjs/language-switcher/index.css": "./cjs/language-switcher/index.css",
    "./cjs/plugin.css": "./cjs/plugin.css",
    "./dist/index.css": "./dist/index.css",
    "./dist/index.min.css": "./dist/index.min.css"
  },
  "main": "./cjs/index.cjs",
  "module": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "unpkg": "./dist/index.min.js",
  "jsdelivr": "./dist/index.min.js",
  "files": [
    "README.md",
    "LICENSE",
    "package.json",
    "esm",
    "cjs",
    "dist"
  ],
  "scripts": {
    "build": "ldesign-builder build -f esm,cjs",
    "build:watch": "ldesign-builder build --watch",
    "dev": "ldesign-builder build --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "clean": "rimraf es lib dist coverage .tsbuildinfo",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@ldesign/i18n-core": "workspace:*",
    "@ldesign/shared": "workspace:*",
    "tslib": "^2.6.2"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.0.0",
    "@ldesign/builder": "workspace:*",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/ui": "^3.2.4",
    "@vue/runtime-core": "^3.5.18",
    "@vue/test-utils": "^2.4.4",
    "eslint": "^9.18.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.7.3",
    "vite": "^5.0.12",
    "vitest": "^3.2.4",
    "vue": "^3.4.15",
    "vue-tsc": "^3.0.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": false
    }
  }
}